					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": "products_testing.csv",
						"container": "webscraping"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
